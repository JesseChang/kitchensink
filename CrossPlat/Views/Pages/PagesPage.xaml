<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="CrossPlat.Views.Pages.PagesPage">
	<ContentPage.Content>
        <StackLayout Margin="0,30">
            <Label Text="Types of Pages" HorizontalTextAlignment="Center" FontSize="Large" FontAttributes="Bold" TextColor="Black" />
            <Button Margin="5,10,5,5" Text="Master Detail" BackgroundColor="Silver" TextColor="Black" Clicked="MasterDetail_Clicked" />
            <Button Margin="5,10,5,5" Text="Tab Page" BackgroundColor="Silver" TextColor="Black" Clicked="TabPage_Clicked" />
            <Button Margin="5,10,5,5" Text="Carousel Page" BackgroundColor="Silver" TextColor="Black" Clicked="Carousel_Clicked" />
        </StackLayout>         
	</ContentPage.Content>
</ContentPage>
